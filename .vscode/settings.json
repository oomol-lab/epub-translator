{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,
  "[log]": {
    "editor.wordWrap": "on"
  },
  "files.associations": {
    "*.log": "log",
    "*.jinja": "jinja-xml",
    "*.xml.jinja": "jinja-xml",
    "*.xhtml.jinja": "jinja-html",
    "*.css.jinja": "jinja-css",
    "*.xml": "xml"
  },
  "[jinja-xml]": {
    "editor.wordWrap": "on"
  },
  "[xml]": {
    "editor.wordWrap": "on"
  },
  "cSpell.words": [
    "analyse",
    "analyser",
    "analysers",
    "analysing",
    "apos",
    "arcname",
    "CJKA",
    "clazz",
    "datas",
    "elems",
    "elevatory",
    "elif",
    "endfor",
    "epub",
    "fitz",
    "frombytes",
    "idref",
    "itemref",
    "iterchildren",
    "kwoa",
    "layoutreader",
    "mathml",
    "matplotlib",
    "metas",
    "nsmap",
    "OEBPS",
    "Onnx",
    "OOMOL",
    "opendocument",
    "pbar",
    "Peekable",
    "pixmap",
    "pylint",
    "rects",
    "rootfile",
    "rootfiles",
    "samp",
    "sents",
    "signum",
    "tiktoken",
    "tobytes",
    "Truncatable",
    "updation",
    "venv",
    "virtualenvs",
    "Zeyu",
    "zinfo"
  ],
  "[python]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "charliermarsh.ruff",
      "editor.codeActionsOnSave": {
          "source.organizeImports": "explicit",
          "source.fixAll": "explicit"
      }
  }
}